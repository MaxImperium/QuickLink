{
  "name": "@quicklink/analytics",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules jest --ci --coverage",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "worker": "tsx src/worker-entrypoint.ts",
    "aggregator": "tsx src/aggregator-entrypoint.ts",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "dependencies": {
    "@quicklink/db": "workspace:*",
    "@quicklink/logger": "workspace:*",
    "@quicklink/shared": "workspace:*",
    "@prisma/client": "^5.7.0",
    "bullmq": "^5.0.0",
    "ioredis": "^5.3.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@quicklink/config": "workspace:*",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
