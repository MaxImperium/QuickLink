{
  "name": "@quicklink/redirect",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --dts",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "dependencies": {
    "@quicklink/cache": "workspace:*",
    "@quicklink/logger": "workspace:*",
    "@quicklink/shared": "workspace:*",
    "fastify": "^4.24.0"
  },
  "devDependencies": {
    "@quicklink/config": "workspace:*",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
